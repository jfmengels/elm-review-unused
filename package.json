  "dependencies": {
    "@jfmengels/elm-review": "^1.0.0-beta.2",
    "elm": "^0.19.0-no-deps",
    "elm-format": "^0.8.2",
    "elm-test": "^0.19.0-rev6",
    "elm-xref": "^4.0.0"
  }
